<?xml version="1.0"?>
<robot name="two_finger_gripper" xmlns:xacro="http://www.ros.org/wiki/xacro"> 

  <xacro:property name="finger_mechanical_reduction" value="304.0"/>
  <xacro:property name="finger_motor_torque_constant" value="3.0"/>
  <xacro:property name="joint_effort_limit" value="3.0"/>
  <xacro:property name="joint_velocity_limit" value="2.98452"/>
	<xacro:property name="finger_lower_limit" value="-0.5"/> 
	<xacro:property name="finger_upper_limit" value="0.5"/> 

  <xacro:include filename="$(find rb1_arm_description)/urdf/grippers/two_finger_gripper.gazebo.xacro" />

  <link
    name="base_link_gripper">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.2735" />
      <inertia
        ixx="0.2735"
        ixy="0.0"
        ixz="0.0"
        iyy="0.2735"
        iyz="0.0"
        izz="0.2735" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/base_gripper.dae" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/base_gripper.dae" />
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <link
    name="link_finger1">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.03" />
      <inertia
        ixx="0.03"
        ixy="0.0"
        ixz="0.0"
        iyy="0.03"
        iyz="0.0"
        izz="0.03" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/finger1.dae" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/finger1.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint
    name="jg1"
    type="revolute">
    <origin
      xyz="0.097993 0.028575 0"
      rpy="3.0028E-16 5.9154E-22 2.2204E-16" />
    <parent
      link="base_link_gripper" />
    <child
      link="link_finger1" />
    <axis
      xyz="0 0 -1" />
    <limit effort="${joint_effort_limit}" velocity="${joint_velocity_limit}" upper="${finger_upper_limit}" lower="${finger_lower_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>    
  </joint>
  
  <transmission name="jg1_trans">
	 <type>transmission_interface/SimpleTransmission</type>
     <joint name="jg1" />
     <actuator name="jg1_motor">
	    <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>${finger_mechanical_reduction}</mechanicalReduction>
		<motorTorqueConstant>${finger_motor_torque_constant}</motorTorqueConstant>
	 </actuator>	 
  </transmission>

  <link
    name="link_finger2">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.03" />
      <inertia
        ixx="0.03"
        ixy="0.0"
        ixz="0.0"
        iyy="0.03"
        iyz="0.0"
        izz="0.03" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/finger2.dae" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/finger2.dae" />
      </geometry>
    </collision>
  </link>
  <joint
    name="jg2"
    type="revolute">
    <origin
      xyz="0.097993 -0.028575 0"
      rpy="3.0028E-16 7.477E-22 -5.1764E-15" />
    <parent
      link="base_link_gripper" />
    <child
      link="link_finger2" />
    <axis
      xyz="0 0 1" />
    <limit effort="${joint_effort_limit}" velocity="${joint_velocity_limit}" upper="${finger_upper_limit}" lower="${finger_lower_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>    
  </joint>

  <transmission name="jg2_trans">
	 <type>transmission_interface/SimpleTransmission</type>
     <joint name="jg2" />
     <actuator name="jg2_motor">
	    <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>${finger_mechanical_reduction}</mechanicalReduction>
		<motorTorqueConstant>${finger_motor_torque_constant}</motorTorqueConstant>
	 </actuator>	 
  </transmission>
  
  <!-- Grasp link GRIPPER  -->
	<link name="gripper_grasp_link1">
	</link>  

	<joint name="gripper_grasp_joint1" type="fixed">
		<origin xyz="0.14 0 0" rpy="0.0 0.0 0.0" />
		<parent link="base_link_gripper"/>
		<child link="gripper_grasp_link1"/>
	</joint>

  <!-- TCP management -->
  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <!-- xacro:include filename="$(find rb1_arm_description)/urdf/tcp.urdf.xacro" / -->

</robot>
