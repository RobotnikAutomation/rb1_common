<?xml version="1.0"?>
<robot name="two_finger_gripper" xmlns:xacro="http://www.ros.org/wiki/xacro"> 

  <xacro:property name="finger_mechanical_reduction" value="304.0"/>
  <xacro:property name="finger_motor_torque_constant" value="3.0"/>
  <xacro:property name="joint_effort_limit" value="3.0"/>
  <xacro:property name="joint_velocity_limit" value="2.98452"/>
	<xacro:property name="finger_lower_limit" value="-1.57"/> 
	<xacro:property name="finger_upper_limit" value="0.0"/> 

  <xacro:include filename="$(find rb1_arm_description)/urdf/grippers/rb1_gripper_1.gazebo.xacro" />

  <link
    name="base_link_gripper">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.257" />
      <inertia
        ixx="0.000131356"
        ixy="0.0"
        ixz="0.000000083"
        iyy="0.000130775"
        iyz="0.0"
        izz="0.00006918" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/v1/gripper_motor.dae" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/v1/gripper_motor.dae" />
      </geometry>
      <contact_coefficients kd="1.0" kp="1000.0" mu="0"/>
    </collision>
  </link>

  <link
    name="link_finger1">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.1264" />
      <inertia
        ixx="0.000097351"
        ixy="0.000056737"
        ixz="0.000000245"
        iyy="0.000315015"
        iyz="0.000001224"
        izz="0.000319853" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/v1/gripper_link1.dae" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/v1/gripper_link1.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint
    name="jg1"
    type="fixed">
    <origin
      xyz="0.0 0.0 0"
      rpy="0.0 0.0 0.0" />
    <parent
      link="base_link_gripper" />
    <child
      link="link_finger1" />
  </joint>
  

  <link
    name="link_finger2">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.0633" />
      <inertia
        ixx="0.000062574"
        ixy="-0.00005905"
        ixz="0.000009803"
        iyy="0.000292473"
        iyz="-0.00000366"
        izz="0.00027232" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/v1/gripper_link2.dae" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://rb1_arm_description/meshes/grippers/v1/gripper_link1.dae" />
      </geometry>
    </collision>
  </link>
  <joint
    name="jg2"
    type="revolute">
    <origin
      xyz="0.0 0.0 0"
      rpy="0.0 0.0 0.0" />
    <parent
      link="base_link_gripper" />
    <child
      link="link_finger2" />
    <axis
      xyz="0 0 1" />
    <limit effort="${joint_effort_limit}" velocity="${joint_velocity_limit}" upper="${finger_upper_limit}" lower="${finger_lower_limit}"/>
    <dynamics damping="${joint_damping}" friction="${joint_friction}"/>    
  </joint>

  <transmission name="jg2_trans">
	 <type>transmission_interface/SimpleTransmission</type>
     <joint name="jg2" />
     <actuator name="jg2_motor">
	    <hardwareInterface>EffortJointInterface</hardwareInterface>
        <mechanicalReduction>${finger_mechanical_reduction}</mechanicalReduction>
		<motorTorqueConstant>${finger_motor_torque_constant}</motorTorqueConstant>
	 </actuator>	 
  </transmission>
  

</robot>
